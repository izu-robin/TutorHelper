<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TutorHelper.ViewModel"
                    xmlns:view="clr-namespace:TutorHelper.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>


    <DataTemplate DataType="{x:Type vm:ReportVM}">
        <view:Groups />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LessonsVM}">
        <view:Lessons />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PricingVM}">
        <view:Pricing />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:Settings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StudentsVM}">
        <view:Students />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TextbooksVM}">
        <view:Textbooks />
    </DataTemplate>
    
</ResourceDictionary>